<?xml version="1.0" ?>
<package format="2">
  <name>sumet_perception_msgs</name>
  <version>8.4.0</version>
  <description>
    ROS messages for perception created for the SUMET project and
    the subsequent DSAT, MUER, and RTK projects at Southwest
    Research Institute.
  </description>
  <maintainer email="preed@swri.org">P. J. Reed</maintainer>
  <url type="repository">git@dismount-git.datasys.swri.edu:swri/sumet_messages.git</url>

  <license>United States Government Purpose</license>
  <license>SwRI Proprietary</license>
  <buildtool_depend>catkin</buildtool_depend>
  <depend>sensor_msgs</depend>
  <depend>rosbag</depend>
  <depend>geometry_msgs</depend>
  <build_depend>message_generation</build_depend>
  <exec_depend>message_runtime</exec_depend>
  <export>
    <rosdoc config="rosdoc.yaml"/>
    <rosbag migration_rule_file="msg/GroundSegmentationData.bmr"/>
  </export>
</package>
