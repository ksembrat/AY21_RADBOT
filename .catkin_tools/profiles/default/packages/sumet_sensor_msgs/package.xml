<?xml version="1.0" ?>
<package format="2">
  <name>sumet_sensor_msgs</name>
  <version>8.4.0</version>
  <description>

    The sumet_sensor_msgs package contains the definitions for messages and 
    services related to sensors in the SUMET architecture.

  </description>
  <maintainer email="preed@swri.org">P. J. Reed</maintainer>
  <url type="repository">git@dismount-git.datasys.swri.edu:swri/sumet_messages.git</url>

  <license>United States Government Purpose</license>
  <license>SwRI Proprietary</license>
  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>message_generation</build_depend>
  <depend>std_msgs</depend>
  <depend>marti_sensor_msgs</depend>
  <depend>rosbag</depend>
  <depend>sensor_msgs</depend>
  <exec_depend>message_runtime</exec_depend>
  <export>
    <rosdoc config="rosdoc.yaml"/>
    <rosbag migration_rule_file="msg/imagearray.bmr"/>
  </export>
</package>

